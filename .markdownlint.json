{
	"default": true,
	"blanks-around-fences": false,
	"blanks-around-lists": false,
	"first-line-heading": false,
	"line-length": false,
	"no-hard-tabs": false,
	"no-inline-html": {
		"allowed_elements": ["br", "img", "kbd", "details", "summary"]
	},
	"no-multiple-blanks": {
		"maximum": 2
	},
	"no-trailing-spaces": true,
	"ul-indent": false,
	"ul-style": {
		"style": "consistent"
	}
}
